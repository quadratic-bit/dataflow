{
    "name": "@ashooww/dataflow",
    "version": "0.1.2",
    "description": "TypeScript library for interactive tables",
    "homepage": "https://github.com/quadratic-bit/dataflow",
    "license": "GPL-3.0",
    "author": "ashooww <fynjyxbrjd@gmail.com> (https://github.com/quadratic-bit/)",
    "main": "./dist/dataflow.js",
    "style": "./dist/css/dataflow.css",
    "typings": "./dist/dataflow.d.ts",
    "exports": {
        ".": "./dist/dataflow.js",
        "./fields": "./dist/core/fields.js",
        "./actions": "./dist/core/actions.js",
        "./pagination": "./dist/components/pagination.js",
        "./columns": "./dist/types/columns.js"
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "compile-ts": "tsc && resolve-tspaths",
        "compile-sass": "sass --no-source-map ./src/scss/main.scss:./dist/css/dataflow.css",
        "build": "yarn compile-ts && yarn compile-sass"
    },
    "devDependencies": {
        "resolve-tspaths": "^0.8.17",
        "sass": "^1.69.5",
        "typescript": "^5.2.2"
    }
}
